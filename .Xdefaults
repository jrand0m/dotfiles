! urxvt
URxvt*cursorBlink: 		true
URxvt.font:  xft:Anonymous Pro for Powerline:size=9:antialias=true:autohint=false,xft:IPAGothic:size=11:antialias=true
URxvt.boldFont:  xft:Anonymous Pro for Powerline:size=9:weight=Bold:antialias=true:autohint=false,xft:IPAGothic:size=11:weight=Bold:antialias=true
URxvt.termName: rxvt-unicode
URxvt*scrollBar: 		false
URxvt.perl-ext-common:	default,matcher
URxvt.url-launcher: 	/usr/bin/google-chrome-stable
URxvt.matcher.button: 	1
URxvt*internalBorder: 	16
URxvt.iso14755: 	false
URxvt.iso14755_52: 	false
! Colours ----------------------------------

!*fading:                40
!*fadeColor:             #002b36
!*cursorColor:           #93a1a1
!*pointerColorBackground:#586e75
!*pointerColorForeground:#93a1a1

!--URxvt
URxvt.fading:    30
URxvt.shading:   80
URxvt.scrollBar: 0
URxvt.geometry: 81x22
URxvt.saveLines:1000
URxvt.inputMethod: none 
URxvt.preeditType: OnTheSpot,None
URxvt.imLocale: ja_JP.UTF-8
URxvt.perl-ext-common: default,matcher
URxvt.urlLauncher: google-chrome 
URxvt.matcher.button: 1
URxvt.font:  xft:Anonymous Pro for Powerline:size=11:antialias=true:autohint=false,xft:IPAGothic:size=11:antialias=true
URxvt.boldFont:  xft:Anonymous Pro for Powerline:size=11:weight=Bold:antialias=true:autohint=false,xft:IPAGothic:size=11:weight=Bold:antialias=true
URxvt.termName: rxvt-unicode
URxvt.internalBorder:   0
URxvt.loginShell: true
!URxvt.background: [60]#0d111b
!URxvt.background: [70]#000000
URxvt.transparent: true 
