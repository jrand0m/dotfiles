! urxvt
URxvt*cursorBlink: 		true
URxvt.font:  xft:Anonymous Pro for Powerline:size=9:antialias=true:autohint=false,xft:IPAGothic:size=11:antialias=true
URxvt.boldFont:  xft:Anonymous Pro for Powerline:size=9:weight=Bold:antialias=true:autohint=false,xft:IPAGothic:size=11:weight=Bold:antialias=true
URxvt.termName: rxvt-unicode
URxvt*scrollBar: 		false
URxvt.perl-ext-common:	default,matcher
URxvt.url-launcher: 	/usr/bin/google-chrome-stable
URxvt.matcher.button: 	1
URxvt*internalBorder: 	16
URxvt.iso14755: 	false
URxvt.iso14755_52: 	false
